@model bool

@if (Model)
{
    <form asp-controller="Users" asp-action="Login" method="post" class="navbar-right">
        <ul class="nav navbar-nav">
            <li>
                <button type="submit" class="btn btn-link navbar-btn navbar-link">Log out</button>
            </li>
        </ul>
    </form>
}
else
{
    <ul class="nav navbar-nav">
        <li><a asp-controller="Users" asp-action="Login">Log in</a></li>
    </ul>
}
