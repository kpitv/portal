@model Portal.Presentation.MVC.Assets.ViewModels.AssetTypeViewModel

<div id="createAssetTypeForm">
    <h1>Create Asset Type</h1>
    <form method="post" asp-antiforgery="true">
        <div id="assetTypeName" class="form-group">
            <label asp-for="Name">Name: </label>
            <input asp-for="Name" class="form-control" />
        </div>
        <div id="assetTypeProperties" class="form-group">
            <label>Properties: </label>
            <div class="form-group">
                <input class="form-control" />
                <button type="button" class="btn btn-default removeProperty glyphicon glyphicon-remove"></button>
            </div>
        </div>
        <button type="button" id="addPropertyButton" class="btn btn-default">Add Property</button>
        <button type="button" id="saveAssetType" class="btn btn-default">Save</button>
    </form>
</div>